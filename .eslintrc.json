{
  "plugins": ["prettier"],
  "extends": ["plugin:vue/recommended", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "rules": {
    "semi": [2, "always"],
    "no-console": "off",
    "vue/max-attributes-per-line": "off",
    "no-unused-vars": "off",
    "vue/html-self-closing": "any"
  }
}
